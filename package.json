{
  "name": "@yaseen/m-lib",
  "version": "0.0.4",
  "description": "",
  "type": "commonjs",
  "exports": {
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ]
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "npm": ">=10.2.0 <11.0.0",
    "node": ">=20.10.0 <21.0.0"
  },
  "scripts": {
    "build:prod": "rm -rf ./dist && tsc -p tsconfig.production.json",
    "build:types": "rm -rf ./dist/index.d.ts && tsc -p tsconfig.production.json --emitDeclarationOnly --declaration --outFile ./dist/index.d.ts",
    "package": "npm run lint && npm run build:prod && npm run build:types && cp ./test-configs/e2e/.env ./dist/test-configs/e2e/ && cp ./package-lock.json ./dist/",
    "build": "rm -rf dist-dev && tsc",
    "startup": "npm i && touch .env && code .env && npm start",
    "serve": "node -r source-map-support/register -r dotenv/config dist-dev/index.js",
    "prestart": "npm run build",
    "start": "npm run serve",
    "prewatch": "npm run build",
    "watch": "tsc-watch --onFirstSuccess \"npm run nodemon-watch\"",
    "nodemon-watch": "nodemon --delay 1 --watch dist-dev --watch ./.env --exec \"npm run\" \"nodemon-start\"",
    "nodemon-start": "npm run serve",
    "test:unit": "rm -rf dist-dev && jest --no-cache --runInBand --config ./test-configs/unit/jest.config.js --forceExit --verbose",
    "test:e2e": "rm -rf dist-dev && jest --no-cache --runInBand --config ./test-configs/e2e/jest.config.js --forceExit --verbose",
    "test": "npm run test:unit && npm run test:e2e",
    "test:cov": "rm -rf dist-dev && jest --coverage --no-cache --runInBand --config ./test-configs/e2e/jest.config.js --forceExit --verbose",
    "lint": "eslint . --ext .mjs,.cjs,.js,.ts",
    "postinstall": "if [ -e node_modules ]\nthen\npatch-package\nelse\nrm -rf ./src&&rm -rf ./test-configs&&rm -rf ./e2e&&cp ./dist/package-lock.json ./\nfi",
    "publish:patch": "SEM_VER=`npm version patch` && git push origin ${SEM_VER}",
    "publish:minor": "SEM_VER=`npm version minor` && git push origin ${SEM_VER}",
    "publish:major": "SEM_VER=`npm version major` && git push origin ${SEM_VER}"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.1.1",
    "get-port": "^7.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.1.1",
    "socket.io-client": "^4.7.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@noble/curves": "^1.7.0",
    "@solana/web3.js": "^1.95.5",
    "bs58": "^6.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.9.1",
    "mongoose": "^8.0.3",
    "redis": "^4.6.12",
    "serialize-error": "^11.0.3",
    "signal-exit": "^4.1.0",
    "socket.io": "^4.7.2",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.23.0"
  }
}
